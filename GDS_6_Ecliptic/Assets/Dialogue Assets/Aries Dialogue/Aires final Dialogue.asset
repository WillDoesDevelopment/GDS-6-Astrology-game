  m_Name: Aires final Dialogue
  m_EditorClassIdentifier: 
  line:
  - sentence: "Who\u2019s DISRUPTING ME FROM MY SLUMBER? "
    dialogueType: 0
    AS: {fileID: 8300000, guid: 8c1a6290ecee0a74ab31e9bb440afdba, type: 3}
  - sentence: "That! That would be me\u2026?"
    dialogueType: 1
    AS: {fileID: 0}
  - sentence: 'And who are you?! '
    dialogueType: 0
    AS: {fileID: 0}
  - sentence: "I\u2026oh. I don\u2019t think I know. "
    dialogueType: 1
    AS: {fileID: 8300000, guid: 6a98757c8a90218429b34015d64d3b98, type: 3}
  - sentence: WHAT?! Speak up! You are before the mighty Crius Chrysomallus!
    dialogueType: 0
    AS: {fileID: 8300000, guid: ffb6a85a0bb12e94d963cb6452a8344e, type: 3}
  - sentence: "Eek! Sorry! I- I don\u2019t know who you are, or who I am!!"
    dialogueType: 1
    AS: {fileID: 8300000, guid: a1c9f66915b340a489dc959f2528de15, type: 3}
  - sentence: '*Sniff Sniff.* '
    dialogueType: 0
    AS: {fileID: 0}
  - sentence: "Me? I am the ram that saved two of Poseidons\u2019 Children."
    dialogueType: 0
    AS: {fileID: 8300000, guid: 531e871e20f7aee48a0385bb3f6689b4, type: 3}
  - sentence: "A warrior with impenetrable armor! And you\u2026 "
    dialogueType: 0
    AS: {fileID: 0}
  - sentence: '*Sniff Sniff.*'
    dialogueType: 0
    AS: {fileID: 0}
  - sentence: "\u2026!"
    dialogueType: 0
    AS: {fileID: 8300000, guid: 5f4450cdd037c434aa2a3e48f6a0481c, type: 3}
  - sentence: "You\u2026 may call me Aries. "
    dialogueType: 0
    AS: {fileID: 0}
  - sentence: O-okay. Aries. Sir.
    dialogueType: 1
    AS: {fileID: 0}
  - sentence: 'Do not call me sir! Do not call anyone sir. It is beneath us.  '
    dialogueType: 0
    AS: {fileID: 8300000, guid: de2f0bf69626f414d9e454935ac5eb7f, type: 3}
  - sentence: 'Um.. okay..? '
    dialogueType: 1
    AS: {fileID: 8300000, guid: 6da656922447d814f8d24b317111b929, type: 3}
  - sentence: "\u2026.Us?"
    dialogueType: 1
    AS: {fileID: 0}
  - sentence: 'You still have much work to do. Go forth! '
    dialogueType: 0
    AS: {fileID: 8300000, guid: c2f7459277ab9f44bbdd8450319177ed, type: 3}
